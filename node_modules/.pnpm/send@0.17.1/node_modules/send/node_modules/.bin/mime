#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mime@1.6.0/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules:/home/kusen/nodejs/node_modules:/home/kusen/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
fi
