#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules:/home/kusen/nodejs/node_modules:/home/kusen/node_modules:/home/node_modules:/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/node_modules/mkdirp/bin/cmd.js/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/node_modules/mkdirp/bin/node_modules:/home/kusen/nodejs/upload-download-multer-real-progress/node_modules/.pnpm/node_modules/mkdirp/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
